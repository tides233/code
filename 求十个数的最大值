#include<stdio.h>
int main()
{
	int a, b, c, d, e, f, g, h, i, j;
	
	scanf("%d%d%d%d%d%d%d%d%d%d", &a, &b, &c, &d, &e, &f, &g, &h, &i, &j);
	int arr[] = { a, b, c, d, e, f, g, h, i, j };
	int t = a;
	for (i = 1; i <= 9; i++)
	{
		
		if (t<arr[i])
		{
			t = arr[i];
		}

		
	}
	printf("这十个数中最大的是：%d\n",t);

		return 0;
}
