#define _CRT_SECURE_NO_WARNINGS
int main()
{
	int i = 0;
	int j = 0;
	int sum = 0;
	for (i = 100; i <= 200; i++)
		/*优化1：i<=开根号i
		再优化2：i=101的同时，i+=2*/
	{
		for (j = 2; j <= i - 1; j++)
		{
			if (i%j == 0)
				break;
		}
		if (i == j)
		{
			printf("%d ", i);
			sum++;
		}

	}
	printf("\n%d\n", sum);
	return 0;
}
